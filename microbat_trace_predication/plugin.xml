<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="microbat_mutation_trace"
            id="microbat_mutation_trace.commands.category">
      </category>
      <command
            name="Generate Mutation Trace"
            categoryId="microbat_mutation_trace.commands.category"
            id="microbat_mutation_trace.commands.sampleCommand">
      </command>
      <command
            categoryId="microbat_mutation_trace.commands.category"
            id="microbat_mutation_trace.commands.muregression"
            name="Retrieve From DB">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="microbat_mutation_trace.commands.sampleCommand"
            class="microbat.mutation.trace.handlers.MutationGenerationHandler">
      </handler>
      <handler
            class="microbat.mutation.trace.handlers.MuRegressionRetrieverHandler"
            commandId="microbat_mutation_trace.commands.muregression">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="microbat_mutation_trace.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Generate Mutation Trace"
               mnemonic="M"
               id="microbat_mutation_trace.menus.sampleMenu">
            <command
                  commandId="microbat_mutation_trace.commands.sampleCommand"
                  mnemonic="S"
                  id="microbat_mutation_trace.menus.sampleCommand">
            </command>
             <command
                  commandId="microbat_mutation_trace.commands.muregression"
                  id="microbat_mutation_trace.menus.muregression">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="microbat_mutation_trace.toolbars.sampleToolbar">
            <command
                  commandId="microbat_mutation_trace.commands.sampleCommand"
                  icon="icons/sample.png"
                  tooltip="Generate Mutation Trace"
                  id="microbat_mutation_trace.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="microbat.mutation.trace.preference.MuRegressionPreference"
            id="muregression.preference"
            name="MuRegression">
      </page>
   </extension>
</plugin>
